<p id="notice"><%= notice %></p>

<h1>Rooms</h1>

<ul class="mt-4 list-unstyled">
  <%= render @rooms %>
</ul>

<div class="mt-2">
  <%= render 'rooms/form', room: @room %>
</div>

<br/>

<h2>Online now</h2>

<ul id="onlinenow">
  <% @users.each do |user| %>
    <li><%= user.nickname %></li>
  <% end %>
</ul>
